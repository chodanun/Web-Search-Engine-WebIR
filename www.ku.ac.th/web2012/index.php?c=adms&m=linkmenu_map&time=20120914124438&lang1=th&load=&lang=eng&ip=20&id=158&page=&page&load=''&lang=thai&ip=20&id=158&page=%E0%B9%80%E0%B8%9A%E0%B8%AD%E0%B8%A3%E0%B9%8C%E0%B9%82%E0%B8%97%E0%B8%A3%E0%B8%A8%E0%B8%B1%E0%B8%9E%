function doSearch1() {
				if(document.getElementById('campus').value == '' || document.getElementById('add_list').value == '' || document.getElementById('add_list1').value == ''){
					alert('กรุณาเลือกข้อมูลทั้งหมด');
					return false;
				}else{
               		 document.add.action = 'http://www.ku.ac.th/web2012/index.php?c=adms&m=linkmenu_map1&time=20120914124438&lang1=th&load=&lang=thai&ip=20&id=158&page=&page1=ค้นหาข้อมูลตึก มก.';
